#include "cyBot_Scan.h"
#include "cyBot_uart.h"
#include "lcd.h"
#include "timer.h"

int main(void)
{
    timer_init();
    lcd_init();
    cyBot_uart_init();
    cyBOT_init_Scan(0b0111);

    char message[500];

    cyBOT_Scan_t sensor_data;

    int i =0;
    sprintf(message,"Degrees Distance [cm]");
    while(message[i] != '\0') {
        cyBot_sendByte(message[i]);
        i++;
    }
    cyBot_sendByte('\n');

